<p id="notice"><%= notice %></p>

<h1>Daily Weathers</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Temperature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_weathers.each do |daily_weather| %>
      <tr>
        <td><%= daily_weather.date %></td>
        <td><%= daily_weather.time %></td>
        <td><%= daily_weather.temperature %></td>
        <td><%= link_to 'Show', daily_weather %></td>
        <td><%= link_to 'Edit', edit_daily_weather_path(daily_weather) %></td>
        <td><%= link_to 'Destroy', daily_weather, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Weather', new_daily_weather_path %>
